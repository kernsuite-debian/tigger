+    def dataMinMax(self):
+        if not self._dataminmax:
+            rdata, rmask = self.optimalRavel(self._data)
+            dprint(3, "computing data min/max")
+            try:
+                self._dataminmax = measurements.extrema(rdata, labels=rmask, index=None if rmask is None else False)
+            except:
