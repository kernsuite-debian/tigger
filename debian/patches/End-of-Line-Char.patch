+                            else:
+                                key_pattern = comps[-1], sum(comps[:-1])
+                        else:
+                            if mouse_pattern:
+                                print(
+                                    "WARNING: more than one mouse pattern for function '%s' in mode config section '%s', ignoring. Check your %s." % (
+                                        func, section, ConfigFileName))
+                            else:
+                                mouse_pattern = comps[-1], sum(comps[:-1])
