+    def _getZoomSlice(self, ix, nx):
+        ix0, ix1 = ix - self._radius, ix + self._radius + 1
+        zx0 = -min(ix0, 0)
+        ix0 = max(ix0, 0)
+        zx1 = self._npix - max(ix1, nx - 1) + (nx - 1)
+        ix1 = min(ix1, nx - 1)
+        return ix0, ix1, zx0, zx1
