+class SourceSelectorDialog(QDialog):
+    def __init__(self, parent, flags=Qt.WindowFlags()):
+        QDialog.__init__(self, parent, flags)
+        self.setModal(False)
+        self.setWindowTitle("Select sources by...")
+        lo = QVBoxLayout(self)
