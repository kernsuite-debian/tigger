-                        except:
-                            _dprint(3, 'error loading icon', name, sys.exc_value())
-                            continue
-                        # register pixmap
-                        self._pixmaps[name] = QPixmapWrapper(pm)
-                        nicons += 1
-                        _dprint(4, 'loaded icon', f)
-                    else:
-                        _dprint(4, 'ignoring entry', f)
