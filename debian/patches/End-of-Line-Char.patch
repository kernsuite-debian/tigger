+        # beam size
+        lo1 = QHBoxLayout()
+        lo.addLayout(lo1)
+        lo1.setContentsMargins(0, 0, 0, 0)
+        lo1.addWidget(QLabel("Restoring beam FWHM, major axis:", self))
+        self.wbmaj = QLineEdit(self)
+        lo1.addWidget(self.wbmaj)
