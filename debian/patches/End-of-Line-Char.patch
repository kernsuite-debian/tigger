+                spi = [spi]
+            spi = ",".join(["%.2f" % x for x in spi])
+            self.setColumn(ColumnSpi, src.spectrum.spi, spi)
+            spierr = getattr(src.spectrum, 'spi_err', None)
+            if spierr is not None:
+                if not isinstance(spierr, (list, tuple)):
