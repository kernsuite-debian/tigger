+    def setSkyAxis(self, n, iaxis, nx, x0, dx, xpix0):
+        """Sets the sky axis, n=0 for RA and n=1 for Dec"""
+        if not self.imgslice:
+            raise RuntimeError("setNumAxes() must be called first")
+        # reverse axis if step is negative
